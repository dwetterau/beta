.well.well-sm.message-well.clickable(class=message_info.deleted?'disabled':'', data-href=message_info.link)
  span.message-checkbox
    input.message-checkbox(data-id=message_info.id, type="checkbox", value="")
  span.message.message-name
    = message_info.user
  span.message.message-subject
    = message_info.subject
